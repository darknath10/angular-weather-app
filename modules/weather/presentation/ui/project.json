{
  "name": "modules-weather-presentation-ui",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "modules/weather/presentation/ui/src",
  "prefix": "farm-app",
  "tags": ["type:ui", "scope:weather"],
  "projectType": "library",
  "targets": {
    "test": {
