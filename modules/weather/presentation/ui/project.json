        "jestConfig": "modules/weather/presentation/ui/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "modules/weather/presentation/ui/**/*.ts",
          "modules/weather/presentation/ui/**/*.html"
        ]
      }
    }
  }
}
